{
  "name": "@advanced-rest-client/export-panel",
  "version": "2.0.2",
  "license": "Apache-2.0",
  "description": "Data export panel for Advanced REST Client.",
  "devDependencies": {
    "@polymer/gen-typescript-declarations": "^1.6.1"
  },
  "private": false,
  "main": "export-panel.html",
  "scripts": {
    "lint": "polymer lint export-panel.html",
    "test-sauce": "polymer test --plugin sauce --job-name \"export-panel:local-test\"",
    "test": "polymer test --plugin local",
    "update-types": "gen-typescript-declarations --deleteExisting --outDir .",
    "analyze": "polymer analyze -i export-panel.html > analysis.json",
    "docs": "npm run update-types && npm run analyze"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/export-panel.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/export-panel/issues",
    "email": "arc@mulesoft.com"
  },
  "authors": [
    "Pawel Psztyc",
    "The Advanced REST client authors <arc@mulesoft.com>"
  ]
}
